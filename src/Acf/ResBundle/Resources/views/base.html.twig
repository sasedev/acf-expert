<!DOCTYPE html>
<html>
	<head>
{%block html_head%}
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="author" content="ACEF-expert">
	{%block dyn_meta%}{%if metas is defined%}{%for meta in metas%}
		<meta name="{{meta.name}}" content="{{meta.content|raw}}">
		{%endfor%}{%endif%}
	{%endblock dyn_meta%}
		<link rel="shortcut icon" href="/favicon.ico">
	{%block static_css%}
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	{%endblock static_css%}
	{%block dynamic_css%}
		{%if stylesheets is defined%}{%for css in stylesheets%}
		<link rel="stylesheet" type="text/css" href="/{{css|raw}}">
		{%endfor%}{%endif%}
	{%endblock dynamic_css%}
		<!--[if lt IE 9]>
		<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7/html5shiv.min.js"></script>
		<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/respond.js/1.3.0/respond.min.js"></script>
		<![endif]-->
	{%block begin1_dynamic_js%}
		<script type="text/javascript">
		<!--
	{%block begin1_dynamic_jstxt%}
	{%endblock begin1_dynamic_jstxt%}
		-->
		</script>
	{%endblock begin1_dynamic_js%}
	{%block begin_static_js%}
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js" integrity="sha256-ImQvICV38LovIsvla2zykaCTdEh1Z801Y+DSop91wMU=" crossorigin="anonymous"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/hinclude/0.9.5/hinclude.min.js" integrity="sha256-zT534DpRR4gYRU6uv0bZ744Ito8AZ6pA+YJcpyi5hMc=" crossorigin="anonymous"></script>
	{%endblock begin_static_js%}
	{%block begin_dynamic_js%}
		{%if begin_javascripts is defined%}{%for js in begin_javascripts%}
		<script type="text/javascript" src="/{{js|raw}}"></script>
		{%endfor%}{%endif%}
	{%endblock begin_dynamic_js%}
		<script type="text/javascript">
		<!--
		window.setTimeout(function() { $(".alert").alert('close'); }, 10000);
		{%block begin2_dynamic_jstxt%}
		{%endblock begin2_dynamic_jstxt%}
		-->
		</script>
	{%block headappend%}{%endblock headappend%}
		<title>{%block title%}{%if pagetitle_txt is defined%}{{pagetitle_txt}} - {%endif%}{{sitename}}{%endblock title%}</title>
	</head>
{%endblock html_head%}
{%block html_body%}
<body>
	{%block html_body_content%}
	{%endblock html_body_content%}
	<script type="text/javascript">
	<!--
	{%block end1_dynamic_jstxt%}
	{%endblock end1_dynamic_jstxt%}
	-->
	</script>
	{%block end_static_js%}
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
	{%endblock end_static_js%}
	{%block end_dynamic_js%}
		{%if end_javascripts is defined%}{%for js in end_javascripts%}
	<script type="text/javascript" src="/{{js|raw}}"></script>
		{%endfor%}{%endif%}
	{%endblock end_dynamic_js%}
	<script type="text/javascript">
	<!--
	{%block end2_dynamic_jstxt%}
	{%endblock end2_dynamic_jstxt%}
	-->
	</script>
	{%block endappend%}{%endblock endappend%}
{% if app.environment == 'prod' %}
	<!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-108802298-1"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());
  gtag('config', 'UA-108802298-1');
</script>
{% endif %}
</body>
{%endblock html_body%}
</html>
