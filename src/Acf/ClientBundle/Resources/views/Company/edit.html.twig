						<div class="box-header">
							<h4 class="box-title">{{'Company.tlabel.docgroupsysts.list'|trans}}</h4>
						</div>
						<div class="box-body">
							<div class="table-responsive">
								{% if docgroupsysts|length != 0 %}
								<div id="accordionDocgroupSyst" class="box-group">
									{% for docgroupsyst in docgroupsysts %}
									<div class="panel box box-warning">
										<div class="box-header">
											<h4 class="box-title">
												<a href="#collapse{{ docgroupsyst.id }}" data-parent="#accordionDocgroupSyst" data-toggle="collapse">{{ docgroupsyst.label }}</a>
											</h4>
										</div>
										<div id="collapse{{ docgroupsyst.id }}" class="panel-collapse collapse">
											<div class="row">
												<div class="col-lg-9">&nbsp;</div>
												<div class="col-lg-3">
													<a class="btn btn-sm btn-info" href="{{path('_client_docgroupsyst_editGet',{'uid':docgroupsyst.id})}}">{{ glyphIco('edit') }} {{ 'action.edit'|trans }}</a>
													<a class="btn btn-sm btn-success" href="{{path('_client_docgroupsyst_addDoc',{'uid':docgroupsyst.id})}}">{{ faIco('file-o') }} {{ 'btn.addDoc'|trans }}</a>
												</div>
												{% if docgroupsyst.otherInfos is not empty %}
												<div class="col-lg-12">{{ docgroupsyst.otherInfos | raw }}</div>
												{% endif %}
											</div>
											{% if docgroupsyst.docs|length != 0 %}
											<table class="table table-bordered table-hover dgDocs">
												<thead>
													<tr>
														<th>{{ 'Doc.fileName.label'|trans|raw }}</th>
														<th>{{ 'Doc.mimeType.label'|trans|raw }}</th>
														<th>{{ 'Doc.size.label'|trans|raw }}</th>
														<th>{{ 'Doc.nbrDownloads.label'|trans|raw }}</th>
														<th>{{ 'Doc.dtCrea'|trans|raw }}:</th>
														<th>{{ 'Doc.description.label'|trans|raw }}</th>
														<th>{{ 'action'|trans|raw }}</th>
													</tr>
												</thead>
												<tbody>
													{% for doc in docgroupsyst.docs %}
													<tr>
														<td>{{ doc.originalName }}</td>
														<td>{{ doc.mimeType }}</td>
														<td>
															{%if doc.size<1024%}
															{{doc.size}} o
															{%elseif doc.size<(1024*1024)%}
															{{'%.2f'|format(doc.size/1024)}} Ko
															{%elseif doc.size<(1024*1024*1024)%}
															{{'%.2f'|format(doc.size/1024/1024)}} Mo
															{%else%}
															{{'%.2f'|format(doc.size/1024/1024/1024)}} Go
															{% endif %}
														</td>
														<td>{{ doc.nbrDownloads }}</td>
														<td>{{ doc.dtCrea|date('Y-m-d @ H:i:s') }}</td>
														<td>{{ doc.description | raw }}</td>
														<td>
															<a class="btn btn-sm btn-primary" href="{{path('_client_doc_download',{'uid':doc.id})}}">{{ glyphIco('download') }} {{ 'action.btnDownload'|trans }}</a>
															<a class="btn btn-sm btn-info" href="{{path('_client_doc_editGet',{'uid':doc.id})}}">{{ glyphIco('edit') }} {{ 'action.edit'|trans }}</a>
														</td>
													</tr>
													{% endfor %}
												</tbody>
											</table>
											{% else %}
											<br>
											<br>
											<br>
											{% endif %}
											{% if docgroupsyst.childs|length != 0 %}
												{{ render_hinclude(controller('AcfClientBundle:Docgroupsyst:childs', {parent: docgroupsyst.id})) }}
											{% endif %}
										</div>
									</div>
									{% endfor %}
								</div>
								{% else %}
									{{ 'Docgroupsyst.empty'|trans|raw }}
								{% endif %}
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
